                                  </m:mrow>
                                </m:mstyle>
                              </m:msub>
                            </m:mrow>
                          </m:mfenced>
                        </m:mrow>
                        <m:mrow>
                          <m:mi>P</m:mi>
                          <m:mfenced open="(" close=")">
                            <m:msub>
                              <m:mi>A</m:mi>
                              <m:mstyle fontsize="8pt">
                                <m:mrow>
                                  <m:mn>1</m:mn>
                                </m:mrow>
                              </m:mstyle>
                            </m:msub>
                          </m:mfenced>
                          <m:mi>P</m:mi>
                          <m:mrow>
                            <m:mfenced open="(" close=")">
                              <m:mrow>
                                <m:mi>E</m:mi>
                                <m:mo stretchy="false">âˆ£</m:mo>
                                <m:msub>
                                  <m:mi>A</m:mi>
                                  <m:mstyle fontsize="8pt">
                                    <m:mrow>
                                      <m:mn>1</m:mn>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:msub>
                              </m:mrow>
                            </m:mfenced>
                            <m:mo stretchy="false">+</m:mo>
                            <m:mi>P</m:mi>
                          </m:mrow>
                          <m:mfenced open="(" close=")">
                            <m:msub>
                              <m:mi>A</m:mi>
                              <m:mstyle fontsize="8pt">
                                <m:mrow>
                                  <m:mn>2</m:mn>
                                </m:mrow>
                              </m:mstyle>
                            </m:msub>
                          </m:mfenced>
                          <m:mi>P</m:mi>
                          <m:mrow>
                            <m:mrow>
                              <m:mfenced open="(" close=")">
                                <m:mrow>
                                  <m:mi>E</m:mi>
                                  <m:mo stretchy="false">âˆ£</m:mo>
                                  <m:msub>
                                    <m:mi>A</m:mi>
                                    <m:mstyle fontsize="8pt">
                                      <m:mrow>
                                        <m:mn>2</m:mn>
                                      </m:mrow>
                                    </m:mstyle>
                                  </m:msub>
                                </m:mrow>
                              </m:mfenced>
                              <m:mo stretchy="false">+</m:mo>
                              <m:mo stretchy="false">â‹¯</m:mo>
                            </m:mrow>
                            <m:mo stretchy="false">+</m:mo>
                            <m:mi>P</m:mi>
                          </m:mrow>
                          <m:mfenced open="(" close=")">
                            <m:msub>
                              <m:mi>A</m:mi>
                              <m:mstyle fontsize="8pt">
                                <m:mrow>
                                  <m:mi>n</m:mi>
                                </m:mrow>
                              </m:mstyle>
                            </m:msub>
                          </m:mfenced>
                          <m:mi>P</m:mi>
                          <m:mfenced open="(" close=")">
                            <m:mrow>
                              <m:mi>E</m:mi>
                              <m:mo stretchy="false">âˆ£</m:mo>
                              <m:msub>
                                <m:mi>A</m:mi>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mi>n</m:mi>
                                  </m:mrow>
                                </m:mstyle>
                              </m:msub>
