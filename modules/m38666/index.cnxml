      <exercise id="id1166136328228">
        <problem id="oo-id1166127209505">
          <para id="id1166134018833">If 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mi>P</m:mi><m:mrow><m:mfenced open="(" close=")"><m:mi>E</m:mi></m:mfenced><m:mo stretchy="false">=</m:mo><m:mtext>.</m:mtext></m:mrow><m:mn>9</m:mn></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{P left (E right )= "." 9} {}</m:annotation></m:semantics></m:math>, 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mi>P</m:mi><m:mrow><m:mfenced open="(" close=")"><m:mrow><m:mi>F</m:mi><m:mo stretchy="false">∣</m:mo><m:mi>E</m:mi></m:mrow></m:mfenced><m:mo stretchy="false">=</m:mo><m:mtext>.</m:mtext></m:mrow><m:mtext>36</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{P left (F \lline E right )= "." "36"} {}</m:annotation></m:semantics></m:math>, and 
