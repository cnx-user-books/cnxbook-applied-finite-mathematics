<m:mtable>
<m:mtr>
<m:mtd><m:mrow><m:mi>W</m:mi><m:mrow/></m:mrow></m:mtd>
</m:mtr>
<m:mtr><m:mtd><m:mo>_</m:mo></m:mtd></m:mtr>
<m:mtr><m:mtd><m:mo>_</m:mo></m:mtd></m:mtr></m:mtable></m:mfenced><m:mo stretchy="false">=</m:mo><m:mfenced open="[" close="]"><m:mtable><m:mtr><m:mtd><m:mrow><m:mtext>23</m:mtext><m:mrow/></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mrow><m:mtext>27</m:mtext><m:mrow/></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mtext>27</m:mtext></m:mtd></m:mtr></m:mtable></m:mfenced></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ left [ matrix {
