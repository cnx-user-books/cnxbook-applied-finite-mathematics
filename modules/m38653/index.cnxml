      <exercise id="id1164284019780">
        <problem id="oo-id1164277402888">
          <para id="id1164288237122">Find the third term of 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:msup><m:mfenced open="(" close=")"><m:mrow><m:mn>2x</m:mn><m:mo stretchy="false">−</m:mo><m:mn>3y</m:mn></m:mrow></m:mfenced><m:mstyle fontsize="8pt"><m:mrow><m:mn>6</m:mn></m:mrow></m:mstyle></m:msup></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ left (2x - 3y right ) rSup { size 8{6} } } {}</m:annotation></m:semantics></m:math>.</para>
        </problem>
        <solution id="oo-id1164278437835">
          <equation id="fs-id1168794688506">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>2160</m:mtext><m:msup><m:mi>x</m:mi><m:mstyle fontsize="8pt"><m:mrow><m:mn>4</m:mn></m:mrow></m:mstyle></m:msup><m:msup><m:mi>y</m:mi><m:mstyle fontsize="8pt"><m:mrow><m:mn>2</m:mn></m:mrow></m:mstyle></m:msup></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"2160"x rSup { size 8{4} } y rSup { size 8{2} } } {}</m:annotation></m:semantics></m:math></equation>
        </solution>
      </exercise>
      <exercise id="id1164282953408">
        <problem id="oo-id2721823">
          <para id="id1164277472906">Find the sixth term of 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:msup><m:mfenced open="(" close=")"><m:mrow><m:mn>5x</m:mn><m:mo stretchy="false">+</m:mo><m:mi>y</m:mi></m:mrow></m:mfenced><m:mstyle fontsize="8pt"><m:mrow><m:mn>8</m:mn></m:mrow></m:mstyle></m:msup></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ left (5x+y right ) rSup { size 8{8} } } {}</m:annotation></m:semantics></m:math>.</para>
