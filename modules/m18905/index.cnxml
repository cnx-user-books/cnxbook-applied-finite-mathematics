                            <m:mfrac>
                              <m:mi>r</m:mi>
                              <m:mi>n</m:mi>
                            </m:mfrac>
                          </m:mrow>
                        </m:mfenced>
                        <m:mstyle fontsize="8pt">
                          <m:mrow>
                            <m:mstyle fontstyle="italic">
                              <m:mrow>
                                <m:mtext>nt</m:mtext>
                              </m:mrow>
