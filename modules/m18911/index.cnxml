                      </m:mrow>
                    </m:mtd>
                    <m:mtd>
                      <m:mrow>
                        <m:mtext>10</m:mtext>
                        <m:mi>y</m:mi>
                        <m:mrow/>
                      </m:mrow>
                    </m:mtd>
                    <m:mtd>
                      <m:mo>=</m:mo>                    </m:mtd>
                    <m:mtd>
                      <m:mrow>
                        <m:mtext>27</m:mtext>
