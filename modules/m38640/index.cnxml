          <equation id="fs-id1167379840954">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mrow><m:mi>x</m:mi><m:mo stretchy="false">−</m:mo><m:mn>3y</m:mn></m:mrow><m:mo stretchy="false">+</m:mo><m:mn>2z</m:mn></m:mrow><m:mo stretchy="false">=</m:mo><m:mn>0</m:mn></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{x - 3y+2z=0} {}</m:annotation></m:semantics></m:math></equation>
        
