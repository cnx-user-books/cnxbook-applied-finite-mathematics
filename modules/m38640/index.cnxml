          <list id="fs-id1167376869759" list-type="enumerated" number-style="lower-alpha"><item><para id="id1168072020297"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mtable><m:mtr><m:mtd><m:mrow><m:mi>x</m:mi><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mrow><m:mo stretchy="false">+</m:mo><m:mrow/></m:mrow><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mn>2y</m:mn><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow/></m:mtd><m:mtd><m:mrow/></m:mtd><m:mtd><m:mo>=</m:mo></m:mtd><m:mtd><m:mrow><m:mn>4</m:mn><m:mrow/></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mrow><m:mn>2x</m:mn><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mrow><m:mo stretchy="false">+</m:mo><m:mrow/></m:mrow><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mn>4y</m:mn><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow/></m:mtd><m:mtd><m:mrow/></m:mtd><m:mtd><m:mo>=</m:mo></m:mtd><m:mtd><m:mrow><m:mn>8</m:mn><m:mrow/></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mrow><m:mn>3x</m:mn><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mrow><m:mo stretchy="false">+</m:mo><m:mrow/></m:mrow><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mn>6y</m:mn><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mrow><m:mo stretchy="false">−</m:mo><m:mrow/></m:mrow><m:mrow/></m:mrow></m:mtd><m:mtd><m:mrow><m:mn>3z</m:mn><m:mrow/></m:mrow></m:mtd><m:mtd><m:mo>=</m:mo></m:mtd><m:mtd><m:mrow><m:mn>3</m:mn><m:mrow/></m:mrow></m:mtd></m:mtr></m:mtable></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ matrix {
